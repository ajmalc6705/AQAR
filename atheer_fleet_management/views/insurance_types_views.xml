<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="view_vehicle_insurance_types_tree" model="ir.ui.view">
        <field name="name">insurance.types.tree</field>
        <field name="model">insurance.types</field>
        <field name="arch" type="xml">
            <tree string="Insurance types" sample="1" editable="top">
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id="action_vehicle_insurance_types_window" model="ir.actions.act_window">
        <field name="name">Insurance Types</field>
        <field name="res_model">insurance.types</field>
        <field name="view_mode">tree</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Click to create a New Insurance Types.
            </p>
        </field>
    </record>

    <menuitem id="menu_vehicle_insurance_types"
              action="action_vehicle_insurance_types_window"
              sequence="10"
              name="Insurance Type"
              parent="atheer_fleet_management.menu_master_masters"

    />


</odoo>