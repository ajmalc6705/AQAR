<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="mail_activity_internal_memo" model="mail.activity.type">
            <field name="name">Internal Memo</field>
            <field name="icon">fa-tasks</field>
            <field name="sequence">4</field>
        </record>

        <record id="bank_guarantee_seq" model="ir.sequence">
            <field name="name">Bank Guarantee Sequence</field>
            <field name="code">bank.guarantee.sequence</field>
            <field name="prefix">Bank/Guarantee/</field>
            <field name="padding">3</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

          <record id="asset_location_seq" model="ir.sequence">
            <field name="name">Asset Location Sequence</field>
            <field name="code">asset.location.sequence</field>
            <field name="prefix">Asset/Location/</field>
            <field name="padding">3</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>