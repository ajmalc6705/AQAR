<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="1">
        <record id="portfolio_purchase_seq" model="ir.sequence">
            <field name="name">Portfolio Purchase</field>
            <field name="code">portfolio.purchase.sequence</field>
            <field name="prefix">PO/</field>
            <field name="padding">3</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="portfolio_sale_seq" model="ir.sequence">
            <field name="name">Portfolio sales</field>
            <field name="code">portfolio.sale.sequence</field>
            <field name="prefix">SO/</field>
            <field name="padding">3</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="portfolio_dividend_seq" model="ir.sequence">
            <field name="name">Portfolio Dividend</field>
            <field name="code">portfolio.dividend.sequence</field>
            <field name="prefix">DIV/</field>
            <field name="padding">3</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="portfolio_adjustment_seq" model="ir.sequence">
            <field name="name">Investment Adjustment</field>
            <field name="code">adjustment.sequence</field>
            <field name="prefix">ADJU/</field>
            <field name="padding">3</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>