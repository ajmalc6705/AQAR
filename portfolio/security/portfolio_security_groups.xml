<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record model="ir.module.category" id="module_category_portfolio">
        <field name="name">Portfolio</field>
        <field name="sequence">18</field>
    </record>
    <record id="group_portfolio_user" model="res.groups">
        <field name="name">User</field>
        <field name="category_id" ref="module_category_portfolio"/>
        <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
    </record>
    <record id="group_portfolio_admin" model="res.groups">
        <field name="name">Admin</field>
        <field name="implied_ids" eval="[(4, ref('group_portfolio_user'))]"/>
        <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
        <field name="category_id" ref="module_category_portfolio"/>
    </record>

</odoo>