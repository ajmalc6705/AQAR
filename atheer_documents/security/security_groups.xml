<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record model="ir.module.category" id="module_category_documents_management">
        <field name="name">Document Management</field>
        <field name="sequence">18</field>
    </record>
    <record id="group_doc_user" model="res.groups">
        <field name="name">User</field>
        <field name="category_id" ref="module_category_documents_management"/>
        <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
    </record>
    <record id="group_doc_admin" model="res.groups">
        <field name="name">Admin</field>
        <field name="implied_ids" eval="[(4, ref('atheer_documents.group_doc_user'))]"/>
        <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
        <field name="category_id" ref="module_category_documents_management"/>
    </record>

</odoo>