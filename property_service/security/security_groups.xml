<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record model="ir.module.category" id="module_category_property_service">
        <field name="name">Property Service</field>
        <field name="sequence">18</field>
    </record>
    <record id="group_service_user_own" model="res.groups">
        <field name="name">User</field>
        <field name="category_id" ref="module_category_property_service"/>
        <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
    </record>
     <record id="group_service_user_all" model="res.groups">
        <field name="name">All Contracts</field>
        <field name="implied_ids" eval="[(4, ref('group_service_user_own'))]"/>
        <field name="category_id" ref="module_category_property_service"/>
        <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
    </record>
    <record id="group_service_admin" model="res.groups">
        <field name="name">Admin</field>
        <field name="implied_ids" eval="[(4, ref('group_service_user_own'))]"/>
        <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
        <field name="category_id" ref="module_category_property_service"/>
    </record>

</odoo>