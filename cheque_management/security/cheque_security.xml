<?xml version="1.0" encoding="utf-8"?>
<odoo>

        <record model="ir.module.category" id="module_category_cheque_management">
            <field name="name">Cheque Management</field>
            <field name="description">User access levels for Cheque Management module</field>
        </record>

        <record id="group_officer" model="res.groups">
            <field name="name">Officer</field>
            <field name="category_id" ref="module_category_cheque_management"/>
            <field name="implied_ids" eval="[(6, 0, [ref('base.group_user')])]"/>
        </record>

        <record id="group_finance_manager" model="res.groups">
            <field name="name">CFO</field>
            <field name="category_id" ref="module_category_cheque_management"/>
            <field name="implied_ids" eval="[(6, 0, [ref('base.group_user')])]"/>
        </record>

        <record id="group_owner" model="res.groups">
            <field name="name">Owner</field>
            <field name="category_id" ref="module_category_cheque_management"/>
            <field name="implied_ids" eval="[(6, 0, [ref('base.group_user')])]"/>
        </record>
         <record id="group_receive_pdc_notifications" model="res.groups">
            <field name="name">PDC Notification</field>
            <field name="category_id" ref="module_category_cheque_management"/>
            <field name="implied_ids" eval="[(6, 0, [ref('base.group_user')])]"/>
        </record>
</odoo>
